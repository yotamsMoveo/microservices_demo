      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 443
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: /Users/yotamsaadon/Projects/webapi_yotam_sln/webapi_yotam
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: /Users/yotamsaadon/vsdbg/vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: /Users/yotamsaadon/.aspnet/https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: /Users/yotamsaadon/.microsoft/usersecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: /Users/yotamsaadon/.nuget/packages/
      target: /root/.nuget/packages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: /Users/yotamsaadon/Projects/webapi_yotam_sln
      target: /src
      bind:
        create_host_path: true
